<FindBugsFilter>
    <!-- The sawOpcode method is just ugly. -->
    <Match>
        <Method name="sawOpcode"/>
        <Bug pattern="CC_CYCLOMATIC_COMPLEXITY"/>
    </Match>
    <!-- Bug detectors are stateful across callback methods. -->
    <Match>
        <Package name="com.mebigfatguy.fbcontrib.detect"/>
        <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR"/>
    </Match>
</FindBugsFilter>
